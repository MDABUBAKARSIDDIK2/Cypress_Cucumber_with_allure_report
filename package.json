{
  "name": "cypress-cucumber-boilerplate",
  "version": "2.0.0",
  "description": "Rokomari",
  "main": "index.js",
  "scripts": {
    "cy:run": "cypress run --headed --browser chrome",
    "cy:run:allure": "cypress run --headed --browser chrome --env allure=true",
    "cy:parallel": "cypress-parallel -s cy:run:allure -t 4 -d 'cypress/e2e/features/*.feature' -a false"
  },
  "keywords": [
    "cypress"
  ],
  "author": "cucumber",
  "license": "ISC",
  "devDependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^12.2.0",
    "@bahmutov/cypress-esbuild-preprocessor": "^2.1.5",
    "@shelex/cypress-allure-plugin": "^2.35.2",
    "chai": "^4.3.7",
    "cypress": "^10.11.0",
    "cypress-file-upload": "^5.0.8",
    "cypress-xpath": "^2.0.1",
    "esbuild": "^0.14.54",
    "mocha": "^10.0.0",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.2.1",
    "multiple-cucumber-html-reporter": "^1.21.4"
  },
  "dependencies": {
    "allure-commandline": "^2.18.1",
    "cypress-parallel": "^0.12.0",
    "mocha-allure-reporter": "^1.4.0",
    "mochawesome-report-generator": "^6.2.0"
  }
